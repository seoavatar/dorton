      while ($row = $stmt->fetch(PDO::FETCH_ASSOC)) {

        $t_after = $row['t_after'];
        $t_before = $row['t_before'];
        $start = $row['time'];
        $end = $row['endtime'];
        $start_before = date("Y-m-d H:i", strtotime("-" . $t_after . " minutes", strtotime($start)));
        $start_after = date("Y-m-d H:i", strtotime("+" . $t_before . " minutes", strtotime($start)));

        $end_before = date("Y-m-d H:i", strtotime("-" . $t_after . " minutes", strtotime($end)));
        $end_after = date("Y-m-d H:i", strtotime("+" . $t_before . " minutes", strtotime($end)));

        $result = $row['result'];
        if (($start_before <= $thestime && $start_after >= $thestime) or ($end_before <= $thestime && $end_after >= $thestime)) {

          if ($ctime <= "01:00") {
          } else {
            $mdate = $row['modified'];
            $Datesss = strtotime($mdate);
            $newformat = date('Y-m-d', $Datesss);
            $strip = $newformat;
            $time1 = "00:00";
            $time2 = "01:00";

            if ($ctime >= $time1 && $ctime <= $time2) {
              $result = $row['result'];
            } else {
              if ($strip >= $cdate) {
                $result = $row['result'];
              } else {
                $result = "Loading...";
              }
            }
          }
          if ($result == null) {
            $result = $row['result'];
