      $query = "SELECT * FROM `market_sequence`";
      $stmt = $con->prepare($query);
      $stmt->execute();
      $select_m_row = $stmt->fetch(PDO::FETCH_ASSOC);
      $sequence = explode(',', $select_m_row['sequence']);
      $sequence1 = array_reverse($sequence);

      // include database connection
      //include 'config/database.php';
      // paginate records
      // PAGINATION VARIABLES
      // select data for current page
      $query = "SELECT * FROM live_games WHERE id IN  (" . implode(',', $sequence1) . ")";
      $stmt = $con->prepare($query);
      $stmt->execute();
      // this is how to get number of rows returned
      $num = $stmt->rowCount();
      // link to create record form
      //check if more than 0 record found
      if ($num > 0) {
        while ($row = $stmt->fetch(PDO::FETCH_ASSOC)) {
          // extract row
          // this will make $row['firstname'] to
          // just $firstname only
          $items[] = $row;
        }
        $sequence1 = array_reverse($sequence1, true);

        foreach ($sequence1 as $seq) {

          foreach ($items as $item) {
            if ($seq == $item['id']) {

              $time = date('h:i A', strtotime($item['time']));
              $endtime = date('h:i A', strtotime($item['endtime']));


              echo "<div class='line' style='background-color:{$item['bgcolor']};'>
                 <span style='font-size:24px;'>
                  <span style='color:{$item['mnc']};'>{$item['mname']}
                  </span>
                   <br />
                  <span style='color:{$item['rc']};'>{$item['result']}
                  </span>
                  </span><br/> 
                  <span style='font-size:14px;'>
                  <span style='color:#804000;'>({$time} - {$endtime})  
                     </span>
                  </span>";
              if ($item['description'] != "") {
                echo "
                    <HR>
                 <pre style='color:{$item['dcolor']}; white-space: inherit; white-space: -moz-pre-wrap; white-space: -pre-wrap; white-space: -o-pre-wrap; word-wrap: break-word; '>{$item['description']}</pre>";
              }
              if($item['panelName'] && $item['panelLink']){
                  echo "
                  <br><span class='panel_btn'><a href='{$item['panelLink']}' alt='{$item['panelName']}'>{$item['panelName']}</a></span>";
                }
              echo " </font>
              </div>";
            }
          }
        }
        $stmt = null;
      }
      // if no records found
      else {
        echo "<div class='alert alert-danger'>No records found.</div>";
      }
      ?>
