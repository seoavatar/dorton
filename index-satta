    <div class="satta-matka-block_strt_adsen2">
      <?php
      include 'config/database.php';

      $query = "SELECT * FROM opentoclose";
      $stmt = $con->prepare($query);
      $stmt->execute();
      // this is how to get number of rows returned
      $num = $stmt->rowCount();
      // link to create record form
      //check if more than 0 record found
      if ($num > 0) {
        while ($row = $stmt->fetch(PDO::FETCH_ASSOC)) {
          // extract row
          // this will make $row['firstname'] to $row['value']
          // just $firstname only

          $color = $row['mnc'] == "#A00247" ? "WHITE"  : "#000000";
          $adsDisplay = "";
          if ($row['id']  % 2 == 0) {
            if ($row['id'] == 2) {
              $adsDisplay = $ads1;
            }
            if ($row['id'] == 4) {
              $adsDisplay = $ads2;
            }
            if ($row['id'] == 6) {
              $adsDisplay = $ads3;
            }
            if ($row['id'] == 8) {
              $adsDisplay = $ads1;
            }
          } else {
            $adsDisplay = "";
          }


          echo "
<div style='background-color:{$row['mnc']}; text-align:center; border-bottom: 2px solid {$row['mnc']}; font-weight:bold;font-style:normal;line-height:1.3em;font-size:28px; margin-bottom:4px;'>
<span style='color:{$color};'><u>{$row['mname']}</u><br />
<span style='font-size:20px;'>OTC - {$row['open']}<br />
JODI - {$row['jodi']}<br />
PANNA- {$row['patti']}<br />
SUCCESS -:</span></span></div>
";
          echo $adsDisplay;
        }
        $stmt = null;
      }
      // if no records found
      else {
        echo "<div class='alert alert-danger'>No records found.</div>";
      }
      ?>
    </div>
  </div>
